{
  "expo": {
    "name": "123Ferry",
    "slug": "123ferry-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/adaptive-icon.png",
    "scheme": "123ferryapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.x123ferryapp"
    },
    "web": {
      "output": "static",
      "favicon": "./src/assets/images/adaptive-icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./src/assets/images/splash-icon.png"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0d74a937-37b7-43a1-a7aa-cdb24c49e238"
      }
    }
  }
}
